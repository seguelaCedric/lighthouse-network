{
  "name": "@lighthouse/ai",
  "version": "0.1.0",
  "private": true,
  "description": "AI functionality for Lighthouse Crew Network",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./index.ts",
  "exports": {
    ".": {
      "types": "./index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./brief-parser": {
      "types": "./brief-parser/index.ts",
      "import": "./dist/brief-parser/index.mjs",
      "require": "./dist/brief-parser/index.js"
    },
    "./matcher": {
      "types": "./matcher/index.ts",
      "import": "./dist/matcher/index.mjs",
      "require": "./dist/matcher/index.js"
    },
    "./agentic-search": {
      "types": "./agentic-search/index.ts",
      "import": "./dist/agentic-search/index.mjs",
      "require": "./dist/agentic-search/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest"
  },
  "dependencies": {
    "ai": "^3.4.0",
    "@ai-sdk/anthropic": "^0.0.50",
    "@ai-sdk/openai": "^0.0.60",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@supabase/supabase-js": "^2.39.0"
  }
}
